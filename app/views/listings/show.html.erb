

<div class="modal" id="myModal">
  <div class="modal-header">
    <a class="close" data-dismiss="modal">Ã—</a>
    <h3><%= @listing.street_address %></h3>
  </div>

  <div class="modal-body">
    <% @listing.listing_images.each do |image| %>
      <div class="row">
        <%= image_tag(image.image.url, :class => "span5 offset1 detail-image" )%>
      </div>
    <% end %>
  </div>

  <div class="modal-footer">
    <%= link_to 'Not Interested', listing_path(@listing), :method => "delete", :class => "btn btn-danger" %>
    <%= link_to 'Interested', interested_path(@listing), :method => "put", :class => "btn btn-primary" %> 
  </div>
</div>
